{
  "name": "nextarter-chakra",
  "version": "0.1.0",
  "private": true,
  "author": "sozonome",
  "scripts": {
    "dev": "next dev",
    "generate": "graphql-codegen --require dotenv/config --config codegen.yml",
    "generate:dev": "graphql-codegen --require dotenv/config --config codegen.yml --watch",
    "build": "next build",
    "postbuild": "next-sitemap --config next-sitemap.config.js",
    "start": "next start",
    "lint": "next lint",
    "lint:fix": "eslint src --fix && yarn format",
    "type-check": "tsc --noEmit",
    "format": "prettier --write src",
    "up": "yarn upgrade-interactive",
    "up-latest": "yarn up --latest",
    "release": "cross-env HUSKY=0 standard-version",
    "push-release": "git push --follow-tags origin main",
    "prepare": "husky install"
  },
  "dependencies": {
    "@apollo/client": "^3.3.20",
    "@apollo/react-ssr": "^4.0.0",
    "@chakra-ui/react": "^2.2.4",
    "@emotion/react": "^11.10.0",
    "@emotion/styled": "^11.10.0",
    "apollo-upload-client": "^17.0.0",
    "date-fns": "^1.30.1",
    "framer-motion": "^6.5.1",
    "next": "12.2.0",
    "next-seo": "^5.4.0",
    "nookies": "^2.5.2",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-icons": "^4.4.0",
    "react-intl": "^6.0.5"
  },
  "devDependencies": {
    "@commitlint/config-conventional": "^17.0.3",
    "@commitlint/cz-commitlint": "^17.0.3",
    "@graphql-codegen/cli": "^1.17.8",
    "@graphql-codegen/near-operation-file-preset": "^1.18.3",
    "@graphql-codegen/typescript": "^1.22.3",
    "@graphql-codegen/typescript-operations": "^1.18.2",
    "@graphql-codegen/typescript-react-apollo": "^2.3.1",
    "@types/apollo-upload-client": "^14.1.0",
    "@types/react": "^18.0.15",
    "commitizen": "^4.2.4",
    "commitlint": "^17.0.3",
    "cross-env": "^7.0.3",
    "eslint": "^8.19.0",
    "eslint-config-next": "^12.2.0",
    "eslint-config-sznm": "^1.0.0",
    "husky": "^8.0.1",
    "lint-staged": "^13.0.3",
    "next-sitemap": "^3.1.7",
    "prettier": "^2.7.1",
    "standard-version": "^9.5.0",
    "typescript": "^4.7.4"
  }
}
